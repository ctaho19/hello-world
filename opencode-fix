# 1. Create the folder structure
mkdir -p ~/.opencode_overrides/node_modules/@ai-sdk/openai-compatible

# 2. Copy your bundle there
cp /Users/uno201/opencode/opencode-offline-bundle/github-copilot-provider.bundle.cjs ~/.opencode_overrides/node_modules/@ai-sdk/openai-compatible/index.cjs

# 3. Create package.json
cat > ~/.opencode_overrides/node_modules/@ai-sdk/openai-compatible/package.json << 'EOF'
{
  "name": "@ai-sdk/openai-compatible",
  "version": "9999.0.0-local",
  "main": "index.cjs",
  "type": "commonjs"
}
EOF

# 4. Verify it was created
ls -la ~/.opencode_overrides/node_modules/@ai-sdk/openai-compatible
